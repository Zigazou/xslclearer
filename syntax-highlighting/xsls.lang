        <context id="xsls" class="no-spell-check">
            <include>
                <!-- Comments -->
                <context id="comment" style-ref="comment">
                    <start>\/\/</start>
                    <end>$</end>
                </context>

                <!-- Strings -->
                <context id="string" style-ref="string" end-at-line-end="false">
                    <start>[#]?"</start>
                    <end>"</end>
                    <include>
                        <context id="escaped-character1" style-ref="escaped-character">
                            <match>\\[\\"]</match>
                        </context>
                    </include>
                </context>

                <!-- Inplace -->
                <context id="inplace" style-ref="inplace" end-at-line-end="false">
                    <start>\[</start>
                    <end>\]</end>
                    <include>
