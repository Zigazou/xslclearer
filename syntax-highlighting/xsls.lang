        <style id="string"            _name="String"              map-to="def:string"/>
        <style id="statement"         _name="Statement"           map-to="def:statement"/>
        <style id="inplace"           _name="Inplace"             map-to="def:special-constant"/>
        <style id="attribute"         _name="Attribute"           map-to="def:identifier"/>
    </styles>

